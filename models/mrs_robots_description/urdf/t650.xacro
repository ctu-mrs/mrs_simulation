  <xacro:if value="${optionals['enable_ball_holder']}">
    <xacro:ball_holder
      parent="base_link">
    </xacro:ball_holder>
  </xacro:if>
  <!--}-->

  <!-- MBZIRC profiles {-->

  <!-- Wall challenge {-->
  <xacro:if value="${optionals['wall_challenge']}">

    <!-- Garmin {-->
    <xacro:garmin_macro
      namespace="${namespace}"
      parent_link="base_link"
      orientation=""
      x="0.0"
      y="-0.221"
      z="-0.025"
      roll="0"
      pitch="${rad90}"
      yaw="${rad90}"
      >
    </xacro:garmin_macro>
    <!--}-->

    <!-- wide angle bluefox {-->
    <xacro:bluefox_camera_macro
      namespace="${namespace}"
      parent_link="base_link"
      camera_suffix="bluefox_brick"
      frame_rate="100.0"
      hfov="2.05"
      noise="0.007"
      parent_frame_name="${namespace}/fcu"
      camera_frame_name="${namespace}/bluefox_brick_optical"
      sensor_base_frame_name="${namespace}/bluefox_brick"
      x="0.0"
      y="-0.189"
      z="-0.068"
      roll="0"
      pitch="1.57079632679"
      yaw="-1.57079632679"
      >
    </xacro:bluefox_camera_macro>
    <!--}-->

    <!-- optflow bluefox {-->
    <xacro:bluefox_camera_macro
      namespace="${namespace}"
      parent_link="base_link"
      camera_suffix="bluefox_optflow"
      frame_rate="100.0"
      hfov="1.9"
      noise="0.007"
      parent_frame_name="${namespace}/fcu"
      camera_frame_name="${namespace}/bluefox_optflow_optical"
      sensor_base_frame_name="${namespace}/bluefox_optflow"
      x="-0.041"
      y="-0.185"
      z="-0.068"
      roll="0"
      pitch="1.57079632679"
      yaw="0"
      >
    </xacro:bluefox_camera_macro>
    <!--}-->

    <!-- tilted realsense {-->
    <xacro:realsense_macro
      namespace="${namespace}"
      parent_link="base_link"
      camera_suffix="realsense_down"
      parent_frame_name="${namespace}/fcu"
      enable_realistic_realsense="${optionals['use_realistic_realsense']}"
      x="0.15"
      y="0.15"
      z="-0.05"
      roll="-0.9727464"
      pitch="1.2572625"
      yaw="-2.5665787"
      >
    </xacro:realsense_macro>
    <!--}-->

  </xacro:if>
  <!--}-->

  <!-- Fire challenge (blanket dropping) {-->
  <xacro:if value="${optionals['fire_challenge_blanket']}">

    <!-- Garmin {-->
    <xacro:garmin_macro
      namespace="${namespace}"
      parent_link="base_link"
      orientation=""
      x="0.177"
      y="0.0735"
      z="-0.07"
      roll="0"
      pitch="1.57079632679"
      yaw="0"
      >
    </xacro:garmin_macro>
    <!--}-->

    <!-- Rplidar {-->
    <xacro:rplidar_macro
      namespace="${namespace}"
      x="0.0"
      y="0.0"
      z="0.205"
      roll="0"
      pitch="0"
      yaw="0"
      >
    </xacro:rplidar_macro>
    <!--}-->

    <!-- Thermal camera {-->
    <xacro:thermal_camera_macro
      namespace="${namespace}"
      camera_frame_name="thermal/front_optical"
      camera_topic_name="/${namespace}/thermal/front"
      parent_link="base_link"
      camera_suffix="thermal/front"
      frame_rate="14.0"
      hfov="0.575959"
      image_width="32"
      image_height="32"
      x="0.13"
      y="0"
      z="-0.085"
      roll="0"
      pitch="1.0472"
      yaw="0"
      parent_frame_name="${namespace}/fcu"
      sensor_base_frame_name="${namespace}/thermal/front"
      color="Yellow">
    </xacro:thermal_camera_macro>
    <xacro:thermal_camera_macro
      namespace="${namespace}"
      camera_frame_name="thermal/bottom_optical"
      camera_topic_name="/${namespace}/thermal/bottom"
      parent_link="base_link"
      camera_suffix="thermal/bottom"
      frame_rate="14.0"
      hfov="0.575959"
      image_width="32"
      image_height="32"
      x="0.13"
      y="0"
      z="-0.085"
      roll="0"
      pitch="1.5708"
      yaw="0"
      parent_frame_name="${namespace}/fcu"
      sensor_base_frame_name="${namespace}/thermal/bottom"
      color="Yellow">
    </xacro:thermal_camera_macro>
    <!--}-->

  </xacro:if>
  <!--}-->

  <!--}-->

</robot>
