          <pitch>${pitch}</pitch>
          <yaw>${yaw}</yaw>
        </plugin>
      </sensor>
    </gazebo>
    <gazebo reference="${namespace}/camera_${camera_suffix}_link">
      <material>Gazebo/DarkGrey</material>
    </gazebo>
  </xacro:macro>
  <!--}-->

