  <!-- Macro to add a Timepix3 event camera {-->
  <xacro:macro name="timepix3_macro" params="namespace name parent color material thickness max_message_window sensor_suffix *origin">
    <joint name="${name}_joint" type="fixed">
      <xacro:insert_block name="origin" />
      <parent link="${parent}" />
      <child link="${name}_link" />
